{
  "name": "drupal/stripe_registration",
  "type": "drupal-module",
  "description": "Facilitates paid site registration via a Stripe subscription plan.",
  "keywords": ["Drupal"],
  "license": "GPL-2.0+",
  "homepage": "https://www.drupal.org/project/stripe_registration",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://www.drupal.org/project/issues/stripe_registration",
    "source": "http://cgit.drupalcode.org/stripe_registration"
  },
  "repositories": {
      "drupal": {
          "type": "composer",
          "url": "https://packages.drupal.org/8"
      }
  },
  "require": {
    "drupal/stripe_api": "^1.0.0-alpha1"
  },
  "extra": {
      "branch-alias": {
          "dev-master": "1.x-dev"
      }
  },
  "require-dev": {
    "drupal/coder": "^8.3",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.6.0"
  }
}
